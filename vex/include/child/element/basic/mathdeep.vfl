function
float[] create_pdf_fee(
    float values[];
    ) {
    float min = min(values);
    int len = len(values);
    for (int i = 0; i < len; ++i) {
        values[i] -= min;
    }
    float sum = sum(values);
    float pdf[];
    resize(pdf, len);
    for (int i = 0; i < len; ++i) {
        pdf[i] = values[i] / sum;
    }
    return pdf;
}




