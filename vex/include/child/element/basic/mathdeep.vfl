function
float fit(
    float value, omin, omax, nmin, nmax;
    int clampin, clampout;
    ) {
    float result = value;
    if (!clampin && result < omin) {
        result += nmin - omin;
    } else if (!clampout && result > omax) {
        result += nmax - omax;
    } else {
        result = fit(result, omin, omax, nmin, nmax);
    }
    return result;
}

