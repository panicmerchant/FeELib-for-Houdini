function
float normaldistribution(
    float x, mean, var;
    ) {
    return exp(pow(-(x-mean)/var, 2)/2) / var / sqrt(2*PI);
}

function
float N(
    float x, mean, var;
    ) {
    return exp(pow(-(x-mean)/var, 2)/2) / var / sqrt(2*PI);
}


