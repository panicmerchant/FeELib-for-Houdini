function
int factorial(int n;) {
    if(n < 0) {
        error("bad arg value");
    }
    int result = n;
    for(int i = n-1; i > 1; --i) {
        result *= i;
    }
	return max(result, 1);
}

function
int combination(int n,m;) {
    if(n > m || n < 0) {
        error("bad arg value");
    }
    if(n == 0) {
	    return 0;
    }
    int msn = m - n;
    if(n > msn) {
        n = msn;
    }
    int result = 1;
    for(int i = 1; i <= n; ++i) {
        result = result * (msn+i) / i; //先算乘法，因为(m-n+i)/i除不尽 //result *= (m-n+i) / i;
    }
	return result;
}

function
int permutations(
    int n,m;
    ) {
    if(n > m || n < 0) {
        error("bad arg value");
    }
    if(n == 0) {
	    return 0;
    }
    int msn = m - n;
    int result = msn + 1;
    for(int i = msn + 2; i <= m; ++i) {
        result *= i;
    }
	return result;
}




