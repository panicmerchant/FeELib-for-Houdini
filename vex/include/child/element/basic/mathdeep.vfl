        function\
        type pow(\
            const type num;\
            const int exp;\
            ) {\
            if (exp == 0) {\
                return 1;\
            } else if (exp == 1) {\
                return num;\
            } else if (exp == 2) {\
                return square(num);\
            } else if (exp == 3) {\
                return cubic(num);\
            } else {\
                /*type result = num;\
                for (int i = 1; i < exp; ++i) {\
                    result *= num;\
                }\
                return result;*/\
                return type(pow(num, float(exp)));\
            }\
        }\

